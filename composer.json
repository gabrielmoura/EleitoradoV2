{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-intl": "*",
        "ext-pdo": "*",
        "ext-redis": "*",
        "ext-xml": "*",
        "blx32/laravel-medialibrary": "^1.0",
        "consoletvs/charts": "6.*",
        "elibyy/tcpdf-laravel": "^10.0",
        "gabrielmoura/laravel-pennant-redis": "1.0",
        "guzzlehttp/guzzle": "^7.2",
        "http-interop/http-factory-guzzle": "^1.2",
        "laravel-notification-channels/telegram": "^4.0",
        "laravel/cashier": "^14.12",
        "laravel/framework": "^10.10",
        "laravel/horizon": "^5.16",
        "laravel/jetstream": "^3.2",
        "laravel/octane": "^2.1",
        "laravel/pennant": "^1.3",
        "laravel/sanctum": "^3.2",
        "laravel/scout": "^10.2",
        "laravel/slack-notification-channel": "^2.5",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.8",
        "livewire/livewire": "^2.11",
        "maatwebsite/excel": "^3.1",
        "meilisearch/meilisearch-php": "^1.1",
        "php-flasher/flasher-laravel": "^1.13",
        "predis/predis": "^2.2",
        "spatie/calendar-links": "^1.8",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-medialibrary": "^10.0.0",
        "spatie/laravel-permission": "^5.10",
        "tightenco/ziggy": "^1.6"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "brianium/paratest": "^7.2",
        "fakerphp/faker": "^1.9.1",
        "laradumps/laradumps": "^2.2",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "laravel/telescope": "^4.14",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "3PartyPackages/laravel-medialibrary"
        }
    ]
}
